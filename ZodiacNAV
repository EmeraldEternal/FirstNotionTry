				window.open("https://www.notion.so/emeraldeternal-astrology/Leo-9b1dae6e94ab4072a33506fa95e99334?pvs=4").focus();
			else if (id == "virgo")
				window.open("https://www.notion.so/emeraldeternal-astrology/Virgo-5810f3dc2bc34ba29665b6286adf0e6f?pvs=4").focus();
			else if (id == "libra")
				window.open("https://www.notion.so/emeraldeternal-astrology/Libra-b22ee7d3cccf4fbf88ec61c5d525e49e?pvs=4").focus();
			else if (id == "scorpio")
				window.open("https://www.notion.so/emeraldeternal-astrology/Scorpio-b0ca650189b7466bbfd7afaa7c9dd1b3?pvs=4").focus();
			else if (id == "sagittarius")
				window.open("https://www.notion.so/emeraldeternal-astrology/Sagittarius-39cdaee79d0a47169b52dc1cc8ea9904?pvs=4").focus();
			else if (id == "capricorn")
				window.open("https://www.notion.so/emeraldeternal-astrology/Capricorn-3f8643da44b545e98e5802a4022a85ff?pvs=4").focus();
			else if (id == "aquarius")
				window.open("https://www.notion.so/emeraldeternal-astrology/Aquarius-5d60d377d7514de1a1a7be770d4351b9?pvs=4").focus();
			else if (id == "pisces")
