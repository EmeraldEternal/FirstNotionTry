	<script>

		let direction = "vertical";
		if (direction === "vertical") {
			document.head.insertAdjacentHTML("beforeend", `<style>.navMenu a {display: block; text-align:left; margin:7px} i{width:15px; margin-right:5px} .navMenu{margin:3px}</style>`)
		}


		function navigate(id) {

			if (id == 'aries')
				window.open("https://www.notion.so/emeraldeternal-astrology/Aries-66782d8fb12448d3a8ae4c50933ca4bf?pvs=4").focus();
			else if (id == "taurus")
				window.open("https://www.notion.so/emeraldeternal-astrology/Taurus-12952666749f4f87bde89966397fe6ed?pvs=4").focus();
